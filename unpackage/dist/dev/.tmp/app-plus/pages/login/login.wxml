<view class="content" bind:__l="__l"><view class="header"></view><view class="loginBg"><image src="../../static/img/loginBg.png" mode="aspectFit"></image></view><view class="input-group"><view class="input-row"><image src="../../static/img/user.png" mode></image><m-input bind:input="__e" class="m-input" type="text" clearable placeholder=" 请输入用户名称" value="{{account}}" data-event-opts="{{[['^input',[['__set_model',['$0','account','$event'],['']]]]]}}"></m-input></view><view class="input-row"><image src="../../static/img/password.png" mode></image><m-input bind:input="__e" type="password" displayable placeholder=" 请输入用户密码" value="{{password}}" data-event-opts="{{[['^input',[['__set_model',['$0','password','$event'],['']]]]]}}"></m-input></view></view><view class="signAndForget"><view data-event-opts="{{[['tap',[['toPage',['reg']]]]]}}" bindtap="__e">立即注册</view><view data-event-opts="{{[['tap',[['toPage',['forget']]]]]}}" bindtap="__e">忘记密码?</view></view><button data-event-opts="{{[['tap',[['bindLogin',['$event']]]]]}}" class="loginBtn" bindtap="__e">login</button></view>